using System;

namespace OverSurgery
{
    public class Login
    {

        private string username;
        private string password;


        public string Username
        {
            get { return username; }
            set { username = value; }
        }
         
        public string Password
        {
            get { return password; }
            set { password = value; }
        }
        

        
    }
}
